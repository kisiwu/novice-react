{
  "name": "@novice1-react/react-router-paneling",
  "version": "0.2.1",
  "description": "React Router Paneling is a library to use with React Router to create routes where each segment of the path is a panel.",
  "type": "module",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.es.js"
    },
    "./cjs": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.cjs.js"
    },
    "./es": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.es.js"
    }
  },
  "author": "demingongo",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisiwu/novice-react.git",
    "directory": "packages/react-router-paneling"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "novice1",
    "novice1-react",
    "react-router-paneling"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint .",
    "test": "node ./__tests__/react-router-paneling.test.js"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0"
  }
}
